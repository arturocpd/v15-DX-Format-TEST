<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>AAA_validation</fullName>
    <active>true</active>
    <errorConditionFormula>OR (ISNULL(Account.Name), ISNULL(Id))</errorConditionFormula>
    <errorMessage>Account name cannot be null or Id cannot be null</errorMessage>
</ValidationRule>
